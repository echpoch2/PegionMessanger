<Page x:Class="Pigeon_.reg"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Pigeon_"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="reg">

    <Grid>
        <TextBox Text="{Binding email, Mode=TwoWay}" HorizontalAlignment="Left" Height="23" Margin="319,160,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="124"/>
        <TextBox Text="{Binding password, Mode=TwoWay}" HorizontalAlignment="Left" Margin="319,188,0,0" VerticalAlignment="Top" Width="124"/>
        <TextBox Text="{Binding repassword, Mode=TwoWay}" HorizontalAlignment="Left" Margin="319,211,0,0" VerticalAlignment="Top" Width="124"/>
        <TextBox Text="{Binding login, Mode=TwoWay}" HorizontalAlignment="Left" Height="23" Margin="319,132,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="124"/>
        <Button Command="{Binding RegisterCommand}" Content="Button" HorizontalAlignment="Left" Margin="341,234,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="323,294,0,0" TextWrapping="Wrap" Text="{Binding status}" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Page>
